    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.1.0",
    "mocha": "5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
